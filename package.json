{
  "name": "govuk-nodejs-typescript-example",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf dist",
    "lint": "tslint --project tslint.json",
    "start": "npm run build && node ./bin/www",
    "fakeApi": "json-server -p 4000 db.json",
    "test": "npm run lint && mocha tests/unit/**/*.ts --require ts-node/register",
    "coverage": "nyc npm test",
    "check-coverage": "npm run coverage && nyc check-coverage --lines 70",
    "test-e2e": "gulp --gulpfile gulpfile-e2e.js",
    "generate-assets": "gulp generate-assets",
    "audit": "nsp check",
    "test-pa11y": "node ./tests/accessibility/pa11y.js"
  },
  "dependencies": {
    "@types/csurf": "^1.9.35",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "chokidar": "^2.0.3",
    "class-validator": "^0.8.5",
    "compression": "^1.7.2",
    "cookie-parser": "~1.4.3",
    "csurf": "^1.9.0",
    "debug": "~2.6.9",
    "express": "^4.16.3",
    "express-nunjucks": "^2.2.3",
    "govuk-elements-sass": "^3.1.2",
    "govuk_frontend_toolkit": "^7.5.0",
    "govuk_template_jinja": "^0.24.0",
    "helmet": "^3.12.1",
    "html": "^1.0.0",
    "inversify": "^4.13.0",
    "morgan": "~1.9.0",
    "nunjucks": "^3.1.3",
    "reflect-metadata": "^0.1.12",
    "serve-favicon": "~2.4.5",
    "validator": "^10.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/express": "^4.11.1",
    "@types/mocha": "^5.2.0",
    "@types/node": "^10.1.2",
    "@types/supertest": "^2.0.4",
    "archiver": "^2.1.1",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "gulp": "^3.9.1",
    "gulp-sass": "^4.0.1",
    "gulp-webdriver": "^2.0.3",
    "json-server": "^0.12.2",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.0",
    "nsp": "^3.2.1",
    "nyc": "^11.8.0",
    "pa11y": "^5.0.4",
    "pa11y-reporter-html": "^1.0.0",
    "selenium-standalone": "^6.14.0",
    "supertest": "^3.0.0",
    "ts-mockito": "^2.3.0",
    "ts-node": "^6.0.3",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3",
    "wdio-mocha-framework": "^0.5.13",
    "webdriverio": "^4.12.0"
  }
}
